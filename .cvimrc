set configpath = '~/.cvimrc'
let localconfig

set cncpcompletion
set autohidecursor
let completionengines = ["google", "amazon", "imdb", "dogpile"]
let highlight = "#ffff00"
map <C-o> goBack
map <C-i> goForward
map ]b nextTab
map [b previousTab
map <Space>b :buffer<Space>
map gM :bookmarks<Space>
map gS :viewsource&<CR>

map <C-n> nextCompletionResult
imap <C-n> nextCompletionResult

let mapleader = ","
map <leader>ev :settings<CR>
map <leader><leader> f
map <leader>k x

site '*://*.inoreader.com/*' {
      unmap j
      unmap k
}

